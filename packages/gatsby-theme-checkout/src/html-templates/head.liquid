    <script>
        !function(){if('PerformanceLongTaskTiming' in window){var g=window.__tti={e:[]};
        g.o=new PerformanceObserver(function(l){g.e=g.e.concat(l.getEntries())});
        g.o.observe({entryTypes:['longtask']})}}();
    </script>
    <script type="text/javascript">
        vtex.siteTitle = "{{ config.title }}";
        vtex.accountName = "{{ account.site_name }}";
        vtex.googleMapsApiKey = "{{ config.google_maps_api_key }}" || undefined
        vtex.geolocation = "{{ config.geolocation }}" || undefined
        vtex.invoiceAddress = "{{ config.invoiceAddress }}" || undefined
        vtex.leanShipping = "{{ config.leanShipping }}" || undefined
        vtex.gtmId = "{{ config.gtm_id }}"
        vtex.vtexid = vtex.vtexid || {};
        vtex.vtexid.accountId = "{{ account.id }}";
        vtex.vtexid.accountName = "{{ account.site_name }}";
        vtex.checkout.version = "{{version}}";
    </script>

    <!-- build:js /checkout/script/metrics.min.js -->
    <script type="text/javascript" src="/checkout/lib/splunkevents.js"></script>
