    <!-- build:js /checkout/script/ko-templates.min.js -->
    <script type="text/javascript" src="/checkout/script/ko-templates.js"></script>
    <!-- endbuild -->

    <script>document.write($("#message-template").html())</script>

    <!--remove-->
    <script type="text/javascript">
        // In development fallback to pt-BR as {{ page.culture }} will not be interpolated
        $('html').attr('lang', 'pt-BR');

        var hostParts = window.location.host.split('.');
        if (hostParts.length > 0) {
            vtex.accountName = hostParts[0];
            vtex.vtexid.accountName = hostParts[0];
        }

        window.devPayPalPlus = "{{ developing paypal-plus }}".indexOf('developing') === -1
        window.devCardUI = "{{ developing card-ui }}".indexOf('developing') === -1
        vtex.siteTitle = '';
    </script>
    <script>
        (function () {
            window.vtex = window.vtex || {};
            vtex.portal = vtex.portal || {};
            vtex.portal.context = {
                accountId: '0e914df8-c516-45bc-a45a-0aab6482eaf7'
            };
        })()
    </script>
    <script src="{{ 'scripts/vtex-events-all.min.js' | vtex_io: 'portal-ui', 1 }}"></script>
    <script src="{{ 'scripts/vtex-analytics.js' | vtex_io: 'portal-ui', 1 }}"></script>
